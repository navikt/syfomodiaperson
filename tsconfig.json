{
  "compilerOptions": {
    "module": "commonjs",
    "target": "ES2019",
    "lib": ["es2019", "dom"],
    "sourceMap": true,
    "allowJs": true,
    "jsx": "react",
    "moduleResolution": "node",
    "outDir": "/",
    "baseUrl": "src",
    "paths": {
      "@/*": ["*"]
    },
    "strict": true,
    //Todo: Endre til true under og legg inn returntyper i sagas
    "noImplicitAny": false,
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "exclude": [
    "node_modules",
    "build",
    "scripts",
    "acceptance-tests",
    "jest",
    "src/setupTests.ts"
  ],
  "types": ["typePatches"]
}
